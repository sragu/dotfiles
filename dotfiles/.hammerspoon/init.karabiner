#!/bin/sh

cli=/Applications/Karabiner.app/Contents/Library/bin/karabiner

$cli set osx.right_command_to_hyper 1
/bin/echo -n .
$cli set repeat.wait 25
/bin/echo -n .
$cli set repeat.initial_wait 400
/bin/echo -n .
$cli set parameter.keyoverlaidmodifier_timeout 400
/bin/echo -n .
$cli set remap.controlL2controlL_escape 1
/bin/echo -n .
/bin/echo
